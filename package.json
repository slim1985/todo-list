{
    "name": "todo-list",
    "version": "0.0.1",
    "description": "A simple todo list app",
    "scripts": {
        "start": "webpack-dev-server --config webpack.config.dev.js --port 3000 --open",
        "start:dev": "concurrently --kill-others \"npm run emulator:start\" \"pnpm run start\"",
        "start:prod": "webpack-dev-server --config webpack.config.prod.js --port 3000 --open",
        "build:dev": "webpack --config webpack.config.dev.js",
        "build:prod": "webpack --config webpack.config.prod.js",
        "test": "jest --coverage",
        "test:update": "jest -u",
        "linter:check": "eslint .",
        "formatter:check": "prettier --config .prettierrc.js . --check",
        "formatter:fix": "prettier --config .prettierrc.js . --write",
        "prepare": "husky install",
        "components:start": "storybook dev -p 6006",
        "components:build": "storybook build",
        "emulator:start": "firebase emulators:start --import=.\\.firestore --export-on-exit"
    },
    "main": "build/index.js",
    "author": "Sergei Sulimov",
    "license": "MIT",
    "dependencies": {
        "@reduxjs/toolkit": "^2.1.0",
        "@types/react": "^18.2.38",
        "@types/react-dom": "^18.2.17",
        "@types/react-redux": "^7.1.33",
        "firebase": "^10.8.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^9.1.0"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "7.6.6",
        "@storybook/addon-interactions": "7.6.6",
        "@storybook/addon-links": "7.6.6",
        "@storybook/addon-onboarding": "1.0.10",
        "@storybook/addon-styling-webpack": "0.0.6",
        "@storybook/blocks": "7.6.6",
        "@storybook/react": "7.6.6",
        "@storybook/react-webpack5": "7.6.6",
        "@storybook/test": "7.6.6",
        "@testing-library/jest-dom": "^6.4.2",
        "@testing-library/react": "^15.0.2",
        "@types/jest": "^29.5.12",
        "@typescript-eslint/eslint-plugin": "^6.13.1",
        "@typescript-eslint/parser": "^6.13.1",
        "concurrently": "^8.2.2",
        "css-loader": "^6.8.1",
        "dotenv-webpack": "^8.1.0",
        "eslint": "^8.54.0",
        "eslint-plugin-storybook": "^0.6.15",
        "html-webpack-plugin": "^5.5.3",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "postcss": "^8.4.32",
        "postcss-loader": "^7.3.3",
        "postcss-preset-env": "^9.3.0",
        "prettier": "^3.1.0",
        "storybook": "7.6.6",
        "style-loader": "^3.3.3",
        "tailwindcss": "^3.4.0",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "typescript": "5.3.2",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0",
        "react-dom": "^17.0.0 || ^18.0.0"
    }
}
